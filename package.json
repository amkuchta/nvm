{
  "name": "@jchip/nvm",
  "description": "Universal Node Version Manager (nvm) for Windows and Unix",
  "version": "1.1.7",
  "author": "Tom.Huang <hzlhu.dargon@gmail.com>",
  "contributors": [
    "Joel Chen <joel123@gmail.com>"
  ],
  "main": "install.sh",
  "license": "MIT",
  "scripts": {
    "preversion": "echo Do not run npm version, run clap version --major,minor,patch && exit 1",
    "prepublishOnly": "echo Do not run npm publish, run clap release && exit 1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "chalker": "^1.1.2",
    "extract-zip": "jchip/extract-zip",
    "fd-slicer": "jchip/node-fd-slicer",
    "lodash": "^4.17.11",
    "mkdirp": "*",
    "needle": "^2.4.0",
    "nix-clap": "^1.3.10",
    "rmdir": "*",
    "tar": "^4.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "electrode-archetype-njs-module-dev": "^3.0.1",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "xclap": "^0.2.33",
    "zipit": "^1.0.2"
  },
  "keywords": [
    "Node",
    "version",
    "manager",
    "unix",
    "*nix",
    "windows",
    "nvm",
    "universal",
    "universal nvm"
  ],
  "files": [
    "bin",
    "dist",
    "install.sh",
    "install.ps1"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/nvm.git"
  }
}
