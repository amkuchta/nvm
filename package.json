{
  "name": "@jchip/nvm",
  "description": "nvm: a universal node.js version manager for Windows (no admin) and Unix",
  "version": "1.5.3",
  "author": "Tom.Huang <hzlhu.dargon@gmail.com>",
  "contributors": [
    "Joel Chen <joel123@gmail.com>"
  ],
  "main": "install.sh",
  "license": "MIT",
  "scripts": {
    "test": "clap check",
    "preversion": "echo Do not run npm version, run clap version --major,minor,patch && exit 1",
    "prepublishOnly": "echo Do not run npm publish, run clap release && exit 1",
    "preinstall": "node bin/no-npm-install.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "chalker": "^1.2.0",
    "extract-zip": "jchip/extract-zip",
    "fd-slicer": "jchip/node-fd-slicer",
    "lodash": "^4.17.20",
    "mkdirp": "*",
    "needle": "^2.6.0",
    "nix-clap": "^1.3.12",
    "opfs": "^1.1.1",
    "rimraf": "^2.6.3",
    "tar": "^4.4.10",
    "xaa": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "electrode-archetype-njs-module-dev": "^3.0.3",
    "prettier": "^1.19.1",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "xclap": "^0.2.33",
    "zipit": "^1.0.2"
  },
  "keywords": [
    "Node",
    "version",
    "manager",
    "unix",
    "*nix",
    "windows",
    "nvm",
    "windows nvm",
    "nvm windows",
    "no admin nvm",
    "universal",
    "universal nvm"
  ],
  "files": [
    "bin",
    "dist",
    "install.sh",
    "install.ps1",
    "check-registry.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/nvm.git"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test"
    ],
    "check-coverage": true,
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "cache": true
  },
  "opfs": {
    "mkdirp": true,
    "rimraf": true,
    "bluebird": false
  }
}
